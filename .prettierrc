{
  "printWidth": 100,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "tabWidth": 2,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^next/(.*)$",
    "^lib/(.*)$",
    "^hooks/(.*)$",
    "^components/(.*)$",
    "^components/UI/(.*)$",
    "^public/(.*)$",
    "^styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
